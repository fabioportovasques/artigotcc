@article{CorreiaGustavoRissari2016ADSD,
issn = {1414-3984},
abstract = {Este trabalho apresenta uma proposta de desenvolvimento de um protótipo de baixo custo de aquisição (plataforma Arduino) para monitoramento e controle automático da irrigação, com acionamento remoto via aplicativo WEB. O trabalho consistiu na construção de dispositivo físico (hardware), software (acesso via WEB e para sistema operacional Android) e testes em campo. Utilizou-se de código aberto (open source), de equipamentos de automação e de instrumentos de comunicação remota. O sistema também conta com um conjunto de algoritmos que permitirá o melhor uso dos recursos hídricos a partir de informações coletadas de sensores inseridos no ambiente que estará sujeito à irrigação.},
journal = {Engenharia na agricultura},
pages = {314--325},
volume = {24},
number = {4},
year = {2016},
title = {AUTOMAÇÃO DE SISTEMA DE IRRIGAÇÃO COM MONITORAMENTO VIA APLICATIVO WEB},
language = {eng},
author = {Correia, Gustavo Rissari and Rocha, Helder Roberto de Oliveira and Rissino, Silvia Das Dores},
}

